import java.util.*;
public class two_summation {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a[]= {1,2};
		int b[] = {2,3};
		int sum=0;
		for(int i=0;i<a.length;i++) {
			for(int j=0;j<a.length;j++) {
				sum = sum+ a[i]+b[j];
				System.out.println(sum);
			}
		}
	}

}
